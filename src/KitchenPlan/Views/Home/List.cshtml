@model List<KitchenPlan.Models.PantryItem>
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title></title>
</head>
<body>
<h1>List of Data</h1>
    <div>
    <table border = 1>
    @foreach (var item in this.Model)
    { 
        <tr>
            <td>@item.PantryItemId</td>
            <td>@item.Description</td>
        </tr>
    }
    </table>
    </div>
</body>
</html>
